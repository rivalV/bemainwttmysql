{
  "name": "server",
  "version": "1.0.0",
  "description": "Server app using expressjs",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "lint": "eslint ./",
    "model:generate": "npx sequelize-cli model:generate",
    "migration:generate": "npx sequelize-cli migration:generate",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:generate": "npx sequelize-cli seed:generate",
    "seed:up:all": "npx sequelize-cli db:seed:all",
    "seed:down:all": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "server"
  ],
  "author": "Chalisun Firifqah",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.10.0",
    "sequelize": "^6.37.3"
  }
}
